#trackMap {
  width: 100%;
  height: calc(100vh - 265px);
}

.trackFilter {
  background: #fff;
  border-top: 1rpx solid #d9d9d9;
  font-size: 14px;
}

.flex-container {
  display: flex;
  justify-content: center;
}

.time-btn-group {
  padding: 10px 0;
}

.time-group {
  padding-bottom: 10px;
}

.time-btn-normal {
  color: #92979e;
  padding: 2px 5px;
  margin: 0 10px;
  border-radius: 10px;
  letter-spacing: 2px;
  text-align: center;
  flex-grow: 1;
}

.earlier-time {
  border: 1rpx solid #dfdfdf;
  color: #1e2227;
}

.date-text {
  color: #92979e;
}

.plateNo-container {
  border-top: 1rpx solid #efefef;
  border-bottom: 1rpx solid #efefef;
  padding: 10px;
}

.plateNo-label {
  width: 80px;
  height: 1.4rem;
  line-height: 1.4rem;
  float: left;
  text-align: center;
}

.plateNo-input {
  font-size: 14px;
  width: calc(100vw - 100px);
}

.date-icon {
  margin-right: 10px;
}
.date-icon2{
  width:100rpx;
  text-align:center;
}
.date-icon-base{
  width: 20px;
  height: 20px;
  position: relative;
  top: 16rpx;
}

.border-bottom {
  border-bottom: 1rpx solid #efefef;
}

.playContainer {
  border-top: 1rpx solid #d9d9d9;
  display: flex;
  align-items: center;
}

.car-info {
  width: calc(100% - 15% - 3% - 60px);
  margin-left: 3%;
  float: left;
}

.tb-15 {
  width: 15%;
  display: flex;
  justify-content: center;
  float: left;
}

.tb-50 {
  width: 50%;
  float: left;
}

.tb-100 {
  width: 100%;
}

.play {
  width: 72rpx;
  height: 72rpx;
}

.play image {
  width: 100%;
  height: 100%;
}

.weui-margin {
  margin: 10rpx 0 5rpx;
}

.circle-container {
  float: right;
  width: 60px;
  height: 60px;
}

.circle-wrapper {
  position: relative;
  width: 100%;
  height: 100%;
}

.circle-wrapper .circleBg {
  position: absolute;
  width: 100%;
  height: 100%;
}

.circle-wrapper .circle {
  width: 100%;
  height: 100%;
}

.circle-wrapper .process-text {
  position: absolute;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  width: 40px;
  height: 40px;
  top: 50%;
  left: 50%;
  margin-left: -20px;
  margin-top: -20px;
}

.circle-wrapper .distance {
  padding-top: 5px;
}
